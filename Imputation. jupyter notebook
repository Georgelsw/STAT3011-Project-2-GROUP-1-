   "source": [
    "Since we do not want to waste the data which include Nah in columns (menus.amountMax,menus.amountMin), we replace the nah columns with their max mean(12.4792) and min mean(11.4278) by imputation method."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "id": "30c6978e",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "from sklearn.impute import SimpleImputer\n",
    "\n",
    "mean_imputer = SimpleImputer(strategy='mean')\n",
    "pizza['menus.amountMax'] = mean_imputer.fit_transform(pizza['menus.amountMax'].values.reshape(-1,1))\n",
    "pizza['menus.amountMin'] = mean_imputer.fit_transform(pizza['menus.amountMin'].values.reshape(-1,1))\n"
   ]
  },
